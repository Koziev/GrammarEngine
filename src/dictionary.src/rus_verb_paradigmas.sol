#include "sg_defs.h"

automat sg
{
 /****************************************************************************
  Универсальный макрос-шаблон для создания парадигм глагола. Позволяет создать
  парадигму как для несовершенного, так и для совершенного вида, динамически
  меняя подставляемый текст.
 *****************************************************************************/

 #define глагол(                             \
                Вид, Индекс,                 \
                                             \ // --- предикатные формы ---
                V1, V2,                      \ // ЖУЮ ЖУЁМ
                V3, V4,                      \ // ЖУЁШЬ ЖУЁМ
                V5, V6,                      \ // ЖУЁТ ЖУЮТ
                V7, V8, V9, V10,             \ // ЖЕВАЛ ЖЕВАЛА ЖЕВАЛО ЖЕВАЛИ
                V11, V12,                    \ // ЖУЙ ЖУЙТЕ
                V33=NULL                     \ // ПОЙДЕМТЕ - императив совместного действия, обычно отсутствует
               )                             \
 #begin
 paradigm Глагол_##Индекс : ГЛАГОЛ
 {
  ВИД:Вид
  НАКЛОНЕНИЕ:ИЗЪЯВ |{

  #if Вид==НЕСОВЕРШ
                     ВРЕМЯ:НАСТОЯЩЕЕ |{
  #else
                     ВРЕМЯ:БУДУЩЕЕ   |{
  #endif
                                       ЛИЦО:1 ЧИСЛО { V1 V2 }
                                       ЛИЦО:2 ЧИСЛО { V3 V4 }
                                       ЛИЦО:3 ЧИСЛО { V5 V6 }
                                      }

                     ВРЕМЯ:ПРОШЕДШЕЕ |{
                                       ЧИСЛО:ЕД РОД { V7 V8 V9 }
                                       ЧИСЛО:МН     { V10 }
                                      }
                    }

  НАКЛОНЕНИЕ:ПОБУД ЛИЦО:2 ЧИСЛО { V11 V12 }

  #if V33!=NULL
   НАКЛОНЕНИЕ:ПОБУД ЛИЦО:1 ЧИСЛО:МН { V33 }
  #endif 
 }
 #end

 #define глагол2(                             \
                 Вид, Индекс, Образец, Маска, \
                                              \ // --- предикатные формы ---
                 V1, V2,                      \ // ЖУЮ ЖУЁМ
                 V3, V4,                      \ // ЖУЁШЬ ЖУЁМ
                 V5, V6,                      \ // ЖУЁТ ЖУЮТ
                 V7, V8, V9, V10,             \ // ЖЕВАЛ ЖЕВАЛА ЖЕВАЛО ЖЕВАЛИ
                 V11, V12                     \ // ЖУЙ ЖУЙТЕ
                )                             \
 #begin
  paradigm Образец, Глагол_##Индекс : ГЛАГОЛ for Маска
 {
  ВИД:Вид
  НАКЛОНЕНИЕ:ИЗЪЯВ |{

  #if Вид==НЕСОВЕРШ
                     ВРЕМЯ:НАСТОЯЩЕЕ |{
  #else
                     ВРЕМЯ:БУДУЩЕЕ   |{
  #endif
                                       ЛИЦО:1 ЧИСЛО { V1 V2 }
                                       ЛИЦО:2 ЧИСЛО { V3 V4 }
                                       ЛИЦО:3 ЧИСЛО { V5 V6 }
                                      }

                     ВРЕМЯ:ПРОШЕДШЕЕ |{
                                       ЧИСЛО:ЕД РОД { V7 V8 V9 }
                                       ЧИСЛО:МН     { V10 }
                                      }
                    }

  НАКЛОНЕНИЕ:ПОБУД ЛИЦО:2 ЧИСЛО { V11 V12 }
 }
 #end

 // ---------------------------------------------------------------------------

 глагол2(                                            // ДЕЛАТЬ
         НЕСОВЕРШ, 6001, Делать, "(.+)АТЬ",          // --------------------------
         "%-2%+Ю",   "%-2%+ЕМ",                      // ДЕЛАЮ   ДЕЛАЕМ
         "%-2%+ЕШЬ", "%-2%+ЕТЕ",                     // ДЕЛАЕШЬ ДЕЛАЕТЕ
         "%-2%+ЕТ",  "%-2%+ЮТ",                      // ДЕЛАЕТ  ДЕЛАЮТ
         "%-2%+Л",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // ДЕЛАЛ   ДЕЛАЛА ДЕЛАЛО ДЕЛАЛИ
         "%-2%+Й", "%-2%+ЙТЕ"                        // ДЕЛАЙ ДЕЛАЙТЕ
        )

 глагол2(                                            // БЕЖАТЬ
         НЕСОВЕРШ, 6002, Бежать, "(.+)ЖАТЬ",         // --------------------------
         "%-4%+ГУ",   "%-3%+ИМ",                     // БЕГУ    БЕЖИМ
         "%-3%+ИШЬ", "%-3%+ИТЕ",                     // БЕЖИШЬ  БЕЖИТЕ
         "%-3%+ИТ",  "%-4%+ГУТ",                     // БЕЖИТ   БЕГУТ
         "%-2%+Л",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // БЕЖАЛ   БЕЖАЛА БЕЖАЛО БЕЖАЛИ
         "%-4%+ГИ", "%-4%+ГИТЕ"                      // БЕГИ    БЕГИТЕ
        )

 глагол(                                            // ЛЕЖАТЬ
        НЕСОВЕРШ, 6003,                             // --------------------------
        "%-3%+У",   "%-3%+ИМ",                      // ЛЕЖУ    ЛЕЖИМ
        "%-3%+ИШЬ", "%-3%+ИТЕ",                     // ЛЕЖИШЬ  ЛЕЖИТЕ
        "%-3%+ИТ",  "%-3%+АТ",                      // ЛЕЖИТ   ЛЕЖАТ
        "%-2%+Л",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // ЛЕЖАЛ ЛЕЖАЛА ЛЕЖАЛО ЛЕЖАЛИ
        "%-3%+И", "%-3%+ИТЕ"                        // ЛЕЖИ ЛЕЖИТЕ
       )

 глагол(                                            // ПИТЬ
        НЕСОВЕРШ, 6004,                             // --------------------------
        "%-3%+ЬЮ",   "%-3%+ЬЁМ",                    // ПЬЮ    ПЬЁМ
        "%-3%+ЬЁШЬ", "%-3%+ЬЁТЕ",                   // ПЬЁШЬ  ПЬЁТЕ
        "%-3%+ЬЁТ",  "%-3%+ЬЮТ",                    // ПЬЁТ   ПЬЮТ
        "%-2%+Л",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // ПИЛ ПИЛА ПИЛО ПИЛИ
        "%-3%+ЕЙ", "%-3%+ЕЙТЕ"                      // ПЕЙ ПЕЙТЕ
       )

 глагол(                                               // ЖЕЧЬ
        НЕСОВЕРШ, 6005,                                // --------------------------
        "%-3%+ГУ",   "%-3%+ЖЁМ",                       // ЖГУ   ЖЖЁМ
        "%-3%+ЖЁШЬ", "%-3%+ЖЁТЕ",                      // ЖЖЁШЬ ЖЖЁТЕ
        "%-3%+ЖЁТ",  "%-3%+ГУТ",                       // ЖЖЁТ  ЖГУТ
        "%-3%+ЁГ", "%-3%+ГЛА", "%-3%+ГЛО", "%-3%+ГЛИ", // ЖЁГ ЖГЛА ЖГЛО ЖГЛИ
        "%-3%+ГИ", "%-3%+ГИТЕ"                         // ЖГИ ЖГИТЕ
       )

 глагол(                                               // ЖИТЬ
        НЕСОВЕРШ, 6006,                                // --------------------------
        "%-2%+ВУ",   "%-2%+ВЁМ",                       // ЖИВУ   ЖИВЁМ
        "%-2%+ВЁШЬ", "%-2%+ВЁТЕ",                      // ЖИВЁШЬ ЖИВЁТЕ
        "%-2%+ВЁТ",  "%-2%+ВУТ",                       // ЖИВЁТ  ЖИВУТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // ЖИЛ ЖИЛА ЖИЛО ЖИЛИ
        "%-2%+ВИ", "%-2%+ВИТЕ"                         // ЖИВИ ЖИВИТЕ
       )

 глагол(                                               // СПАТЬ
        НЕСОВЕРШ, 6007,                                // --------------------------
        "%-3%+ЛЮ",   "%-3%+ИМ",                        // СПЛЮ  СПИМ
        "%-3%+ИШЬ", "%-3%+ИТЕ",                        // СПИШЬ СПИТЕ
        "%-3%+ИТ",  "%-3%+ЯТ",                         // СПИТ  СПЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // СПАЛ СПАЛА СПАЛО СПАЛИ
        "%-3%+И", "%-3%+ИТЕ"                           // СПИ СПИТЕ
       )


 глагол2(                                              // СМОТРЕТЬ
        НЕСОВЕРШ, 6010, Смотреть, "(.+)РЕТЬ",          // --------------------------
        "%-3%+Ю",   "%-3%+ИМ",                         // СМОТРЮ СМОТРИМ
        "%-3%+ИШЬ", "%-3%+ИТЕ",                        // СМОТРИШЬ СМОТРИТЕ
        "%-3%+ИТ",  "%-3%+ЯТ",                         // СМОТРИТ  СМОТРЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // СМОТРЕЛ СМОТРЕЛА СМОТРЕЛО СМОТРЕЛИ
        "%-3%+И", "%-3%+ИТЕ"                           // СМОТРИ СМОТРИТЕ
       )

 глагол2(                                              // КОНТРОЛИРОВАТЬ
        НЕСОВЕРШ, 6011, КОНТРОЛИРОВАТЬ, "(.+)ОВАТЬ",   // --------------------------
        "%-5%+УЮ",   "%-5%+УЕМ",                       // КОНТРОЛИРУЮ КОНТРОЛИРУЕМ
        "%-5%+УЕШЬ", "%-5%+УЕТЕ",                      // КОНТРОЛИРУЕШЬ КОНТРОЛИРУЕТЕ
        "%-5%+УЕТ",  "%-5%+УЮТ",                       // КОНТРОЛИРУЕТ КОНТРОЛИРУЮТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // КОНТРОЛИРОВАЛ КОНТРОЛИРОВАЛА КОНТРОЛИРОВАЛО КОНТРОЛИРОВАЛИ
        "%-5%+УЙ", "%-5%+УЙТЕ"                         // КОНТРОЛИРУЙ КОНТРОЛИРУЙТЕ
       )

 глагол(                                               // МЫТЬ
        НЕСОВЕРШ, 6013,                                // --------------------------
        "%-3%+ОЮ",   "%-3%+ОЕМ",                       // МОЮ МОЕМ
        "%-3%+ОЕШЬ", "%-3%+ОЕТЕ",                      // МОЕШЬ МОЕТЕ
        "%-3%+ОЕТ",  "%-3%+ОЮТ",                       // МОЕТ МОЮТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // МЫЛ МЫЛА МЫЛО МЫЛИ
        "%-3%+ОЙ", "%-3%+ОЙТЕ"                         // МОЙ МОЙТЕ
       )

 глагол(                                               // ЛЕТЕТЬ
        НЕСОВЕРШ, 6014,                                // --------------------------
        "%-4%+ЧУ",   "%-3%+ИМ",                        // ЛЕЧУ ЛЕТИМ
        "%-3%+ИШЬ", "%-3%+ИТЕ",                        // ЛЕТИШЬ ЛЕТИТЕ
        "%-3%+ИТ",  "%-3%+ЯТ",                         // ЛЕТИТ ЛЕТЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // ЛЕТЕЛ ЛЕТЕЛА ЛЕТЕЛО ЛЕТЕЛИ
        "%-3%+И", "%-3%+ИТЕ"                           // ЛЕТИ ЛЕТИТЕ
       )


 глагол(                                             // ВИДЕТЬ
        НЕСОВЕРШ, 6017,                              // --------------------------
        "%-4%+ЖУ",   "%-3%+ИМ",                      // ВИЖУ    ВИДИМ
        "%-3%+ИШЬ", "%-3%+ИТЕ",                      // ВИДИШЬ  ВИДИТЕ
        "%-3%+ИТ",  "%-3%+ЯТ",                       // ВИДИТ   ВИДЯТ
        "%-2%+Л",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",  // ВИДЕЛ ВИДЕЛА ВИДЕЛО ВИДЕЛИ
        "%-3%+И", "%-3%+ИТЕ"                         // ГЛЯДИ ГЛЯДИТЕ
       )

 глагол(                                               // СТОЯТЬ
        НЕСОВЕРШ, 6018,                                // --------------------------
        "%-3%+Ю",   "%-3%+ИМ",                         // СТОЮ   СТОИМ
        "%-3%+ИШЬ", "%-3%+ИТЕ",                        // СТОИШЬ СТОИТЕ
        "%-3%+ИТ",  "%-3%+ЯТ",                         // СТОИТ  СТОЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // СТОЯЛ СТОЯЛА СТОЯЛО СТОЯЛИ
        "%-3%+Й", "%-3%+ЙТЕ"                           // СТОЙ   СТОЙТЕ
       )

 глагол(                                               // ГОВОРИТЬ
        НЕСОВЕРШ, 6019,                                // --------------------------
        "%-3%+Ю",   "%-2%+М",                          // ГОВОРЮ ГОВОРИМ
        "%-2%+ШЬ",  "%-1%+Е",                          // ГОВОРИШЬ ГОВОРИТЕ
        "%-1",      "%-3%+ЯТ",                         // ГОВОРИТ ГОВОРЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // ГОВОРИЛ ГОВОРИЛА ГОВОРИЛО ГОВОРИЛИ
        "%-2", "%-1%+Е"                                // ГОВОРИ ГОВОРИТЕ
       )

 глагол(                                               // ПРОСИТЬ
        НЕСОВЕРШ, 6020,                                // --------------------------
        "%-4%+ШУ",   "%-2%+М",                         // ПРОШУ ПРОСИМ
        "%-2%+ШЬ",  "%-1%+Е",                          // ПРОСИШЬ ПРОСИТЕ
        "%-1",      "%-3%+ЯТ",                         // ПРОСИТ ПРОСЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // ПРОСИЛ ПРОСИЛА ПРОСИЛО ПРОСИЛИ
        "%-2", "%-1%+Е"                                // ПРОСИ ПРОСИТЕ
       )

 глагол(                                                     // ХОТЕТЬСЯ
        НЕСОВЕРШ, 6022,                                      // -----------------
        "%-6%+ЧУСЬ",   "%-6%+ЧЕМСЯ",                         // ХОЧУСЬ ХОЧЕМСЯ
        "%-6%+ЧЕШЬСЯ", "%-6%+ЧИТЕСЬ",                        // ХОЧЕШЬСЯ ХОЧИТЕСЬ
        "%-6%+ЧЕТСЯ",  "%-6%+ЧУТСЯ",                         // ХОЧЕТСЯ ХОЧУТСЯ
        "%-4%+ЛСЯ",  "%-4%+ЛАСЬ", "%-4%+ЛОСЬ", "%-4%+ЛИСЬ",  // ХОТЕЛСЯ ХОТЕЛАСЬ ХОТЕЛОСЬ ХОТЕЛИСЬ
        "%-5%+ИСЬ", "%-5%+ИТЕСЬ"                             // ХОТИСЬ ХОТИТЕСЬ
       )

 глагол(                                               // СИДЕТЬ
        НЕСОВЕРШ, 6023,                                // --------------------------
        "%-4%+ЖУ",   "%-3%+ИМ",                        // СИЖУ СИДИМ
        "%-3%+ИШЬ",  "%-3%+ИТЕ",                       // СИДИШЬ СИДИТЕ
        "%-3%+ИТ",   "%-3%+ЯТ",                        // СИДИТ СИДЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // СИДЕЛ СИДЕЛА СИДЕЛО СИДЕЛИ
        "%-3%+И", "%-3%+ИТЕ"                           // СИДИ СИДИТЕ
       )


 paradigm Глагол_6024 : ГЛАГОЛ                                                     // ХОТЕТЬ
 {
  ВИД:НЕСОВЕРШ
  НАКЛОНЕНИЕ:ИЗЪЯВ |{
                     ВРЕМЯ:НАСТОЯЩЕЕ |{
                                       ЛИЦО:1 ЧИСЛО { "%-4%+ЧУ"   "%-4%+ЧЕМ" } // ХОЧУ ХОЧЕМ
                                       ЛИЦО:1 ЧИСЛО:МН { "%-3%+ИМ" }           // ХОТИМ
                                       ЛИЦО:2 ЧИСЛО { "%-4%+ЧЕШЬ" "%-3%+ИТЕ" } // ХОЧЕШЬ ХОТИТЕ
                                       ЛИЦО:3 ЧИСЛО { "%-4%+ЧЕТ"  "%-3%+ЯТ"  } // ХОЧЕТ ХОТЯТ
                                      }

                     ВРЕМЯ:ПРОШЕДШЕЕ |{
                                       ЧИСЛО:ЕД РОД { "%-2%+Л"  "%-2%+ЛА" "%-2%+ЛО" } // ХОТЕЛ ХОТЕЛА ХОТЕЛО
                                       ЧИСЛО:МН     {  "%-2%+ЛИ" }                    // ХОТЕЛИ
                                      }
                    }

  НАКЛОНЕНИЕ:ПОБУД ЛИЦО:2 ЧИСЛО { "%-3%+И" "%-3%+ИТЕ"  } // ХОТИ ХОТИТЕ
 }

 глагол2(                                              // ДАВАТЬ
        НЕСОВЕРШ, 6025, Давать, "(.+)АТЬ",             // --------------------------
        "%-4%+Ю",   "%-4%+ЁМ",                         // ДАЮ ДАЁМ
        "%-4%+ЁШЬ",  "%-4%+ЁТЕ",                       // ДАЁШЬ ДАЁТЕ
        "%-4%+ЁТ", "%-4%+ЮТ",                          // ДАЁТ ДАЮТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // ДАВАЛ ДАВАЛА ДАВАЛО ДАВАЛИ
        "%-2%+Й", "%-2%+ЙТЕ"                           // ДАВАЙ ДАВАЙТЕ
       )


 глагол(                                               // ЗВАТЬ
        НЕСОВЕРШ, 6027,                                // --------------------------
        "%-4%+ОВУ",   "%-4%+ОВЁМ",                     // ЗОВУ ЗОВЁМ
        "%-4%+ОВЁШЬ", "%-4%+ОВЁТЕ",                    // ЗОВЁШЬ ЗОВЁТЕ
        "%-4%+ОВЁТ",  "%-4%+ОВУТ",                     // ЗОВЁТ ЗОВУТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // ЗВАЛ ЗВАЛА ЗВАЛО ЗВАЛИ
        "%-4%+ОВИ", "%-4%+ОВИТЕ"                       // ЗОВИ ЗОВИТЕ
       )

 глагол(                                                   // ЗВАТЬСЯ
        НЕСОВЕРШ, 6028,                                    // --------------------------
        "%-6%+ОВУСЬ",   "%-6%+ОВЁМСЯ",                     // ЗОВУСЬ ЗОВЁМСЯ
        "%-6%+ОВЁШЬСЯ", "%-6%+ОВЁТЕСЬ",                    // ЗОВЁШЬСЯ ЗОВЁТЕСЬ
        "%-6%+ОВЁТСЯ",  "%-6%+ОВУТСЯ",                     // ЗОВЁТСЯ ЗОВУТСЯ
        "%-4%+ЛСЯ", "%-4%+ЛАСЬ", "%-4%+ЛОСЬ", "%-4%+ЛИСЬ", // ЗВАЛСЯ ЗВАЛАСЬ ЗВАЛОСЬ ЗВАЛИСЬ
        "%-6%+ОВИСЬ", "%-6%+ОВИТЕСЬ"                       // ЗОВИСЬ ЗОВИТЕСЬ
       )

 глагол(                                           // ХОДИТЬ
        НЕСОВЕРШ, 6030,                            // --------------------------
        "%-4%+ЖУ",   "%-2%+М",                     // ХОЖУ ХОДИМ
        "%-3%+ИШЬ", "%-2%+ТЕ",                     // ХОДИШЬ ХОДИТЕ
        "%-3%+ИТ",  "%-3%+ЯТ",                     // ХОДИТ ХОДЯТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // ХОДИЛ ХОДИЛА ХОДИЛО ХОДИЛИ
        "%-2", "%-1%+Е"                            // ХОДИ ХОДИТЕ
       )

 глагол(                                           // НАХОДИТЬСЯ
        НЕСОВЕРШ, 6031,                            // --------------------------
