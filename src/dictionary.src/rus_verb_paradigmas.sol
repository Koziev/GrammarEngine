        "%-3%+ТУ",   "%-3%+ТЁМ",                       // ЗАЦВЕТУ ЗАЦВЕТЕМ
        "%-3%+ТЁШЬ", "%-3%+ТЁТЕ",                      // ЗАЦВЕТЕШЬ ЗАЦВЕТЕТЕ
        "%-3%+ТЁТ",  "%-3%+ТУТ",                       // ЗАЦВЕТЕТ ЗАЦВЕТУТ
        "%-4%+ЁЛ", "%-3%+ЛА", "%-3%+ЛО", "%-3%+ЛИ",    // ЗАЦВЕЛ ЗАЦВЕЛА ЗАЦВЕЛО ЗАЦВЕЛИ
        "%-3%+ТИ", "%-3%+ЬТЕ"                          // ЗАЦВЕТИ ЗАЦВЕТИТЕ
       )

 глагол(                                              // ПЕРЕСЕЧЬ
        СОВЕРШ, 7070,                                 // --------------------------
        "%-2%+КУ",   "%-1%+ЁМ",                       // ПЕРЕСЕКУ ПЕРЕСЕЧЁМ
        "%-1%+ЁШЬ", "%-1%+ЁТЕ",                       // ПЕРЕСЕЧЁШЬ ПЕРЕСЕЧЁТЕ
        "%-1%+ЁТ",  "%-2%+КУТ",                       // ПЕРЕСЕЧЁТ ПЕРЕСЕКУТ
        "%-3%+ЁК", "%-2%+КЛА", "%-2%+КЛО", "%-2%+КЛИ",// ПЕРЕСЁК ПЕРЕСЕКЛА ПЕРЕСМЕКЛО ПЕРЕСЕКЛИ
        "%-2%+КИ", "%-2%+КИТЕ"                        // ПЕРЕСЕКИ ПЕРЕСЕКИТЕ
       )

 глагол(                                           // ПЕРЕКРЫТЬ
        СОВЕРШ, 7071,                              // --------------------------
        "%-3%+ОЮ",   "%-3%+ОЕМ",                   // ПЕРЕКРОЮ ПЕРЕКРОЕМ
        "%-3%+ОЕШЬ", "%-3%+ОЕТЕ",                  // ПЕРЕКРОЕШЬ ПЕРЕКРОЕТЕ
        "%-3%+ОЕТ",  "%-3%+ОЮТ",                   // ПЕРЕКРОЕТ ПЕРЕКРОЕТ
        "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // ПЕРЕКРЫЛ ПРЕКРЫЛА ПЕРЕКРЫЛО ПЕРЕКРЫЛИ
        "%-3%+ОЙ", "%-3%+ОЙТЕ"                     // ПЕРЕКРОЙ ПЕРЕКРОЙТЕ
       )


 глагол2(                                            // СВЕРИТЬ
         СОВЕРШ, 7072,                               // -------------------------------
         Сверить, "(.+)РИТЬ",
         "%-3%+Ю",   "%-3%+ИМ",                      // СВЕРЮ   СВЕРИМ
         "%-3%+ИШЬ", "%-3%+ИТЕ",                     // СВЕРИШЬ СВЕРИТЕ
         "%-3%+ИТ",  "%-3%+ЯТ",                      // СВЕРИТ  СВЕРЯТ
         "%-2%+Л",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // СВЕРИЛ СВЕРИЛА СВЕРИЛО СВЕРИЛИ
         "%-3%+Ь", "%-3%+ЬТЕ"                        // СВЕРЬ СВЕРЬТЕ
        )

 глагол(                                              // ЗАКАТИТЬСЯ
        СОВЕРШ, 7073,                                 // -----------------------------
        "%-6%+ЧУСЬ", "%-4%+МСЯ",                      // ЗАКАЧУСЬ  ЗАКАТИМСЯ
        "%-4%+ШЬСЯ", "%-4%+ТЕСЬ",                     // ЗАКАТИШЬСЯ ЗАКАТИТЕСЬ
        "%-3%+СЯ",  "%-5%+ЯТСЯ",                      // ЗАКАТИТСЯ ЗАКАТЯТСЯ
        "%-4%+ЛСЯ",  "%-4%+ЛАСЬ", "%-4%+ЛОСЬ", "%-4%+ЛИСЬ", // ЗАКАТИЛСЯ ЗАКАТИЛАСЬ ЗАКАТИЛОСЬ ЗАКАТИЛИСЬ
        "%-4%+СЬ", "%-4%+ТЕСЬ"                        // ЗАКАТИСЬ ЗАКАТИТЕСЬ
       )

 глагол(                                             // ПРОЧЕСТЬ
        СОВЕРШ, 7074,                                // -------------------------------
        "%-4%+ТУ", "%-4%+ТЁМ",                       // ПРОЧТУ  ПРОЧТЁМ
        "%-4%+ТЁШЬ", "%-4%+ТЁТЕ",                    // ПРОЧТЁШЬ ПРОЧТЁТЕ
        "%-4%+ТЁТ",  "%-4%+ТУТ",                     // ПРОЧТЁТ ПРОЧТУТ
        "%-4%+ЁЛ",  "%-4%+ЛА", "%-4%+ЛО", "%-4%+ЛИ", // ПРОЧЁЛ ПРОЧЛА ПРОЧЛО ПРОЧЛИ
        "%-4%+ТИ", "%-4%+ТИТЕ"                       // ПРОЧТИ ПРОЧТИТЕ
       )

 глагол(                                            // ПРОБЫТЬ
        СОВЕРШ, 7075,                               // -------------------------------
        "%-3%+УДУ", "%-3%+УДЕМ",                    // ПРОБУДУ  ПРОБУДЕМ
        "%-3%+УДЕШЬ", "%-3%+УДЕТЕ",                 // ПРОБУДЕШЬ ПРОБУДЕТЕ
        "%-3%+УДЕТ",  "%-3%+УДУТ",                  // ПРОБУДЕТ ПРОБУДУТ
        "%-2%+Л",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ", // ПРОБЫЛ ПРОЬЫЛА ПРОБЫЛО ПРОБЫЛИ
        "%-3%+УДЬ", "%-3%+УДЬТЕ"                    // ПРОБУДЬ ПРОБУДЬТЕ
       )

 глагол2(                                                   // ПРОДВИНУТЬСЯ
         СОВЕРШ, 7076,                                      // ---------------------
         продвинуться, "(.+)НУТЬСЯ",
         "%-4%+СЬ",   "%-5%+ЕМСЯ",                          // ПРОДВИНУСЬ ПРОДВИНЕМСЯ
         "%-5%+ЕШЬСЯ", "%-5%+ЕТЕСЬ",                        // ПРОДВИНЕШЬСЯ ПРОДВИНЕТЕСЬ
         "%-5%+ЕТСЯ",  "%-3%+СЯ",                           // ПРОДВИНЕТСЯ ПРОДВИНУТСЯ
         "%-4%+ЛСЯ", "%-4%+ЛАСЬ", "%-4%+ЛОСЬ", "%-4%+ЛИСЬ", // ПРОДВИНУЛСЯ ПРОДВИНУЛАСЬ ПРОДВИНУЛОСЬ ПРОДВИНУЛИСЬ
         "%-5%+ЬСЯ", "%-5%+ЬТЕСЬ"                           // ПРОДВИНЬСЯ ПРОДВИНЬТЕСЬ
        )

 глагол(                                            // ЗАЛЕЗТЬ
        СОВЕРШ, 7078,                               // -------------------------------
        "%-2%+У",  "%-2%+ЕМ",                       // ЗАЛЕЗУ  ЗАЛЕЗЕМ
        "%-2%+ЕШЬ", "%-2%+ЕТЕ",                     // ЗАЛЕЗЕШЬ ЗАЛЕЗЕТЕ
        "%-2%+ЕТ", "%-2%+УТ",                       // ЗАЛЕЗЕТ ЗАЛЕЗУТ
        "%-2",  "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",    // ЗАЛЕЗ ЗАЛЕЗЛА ЗАЛЕЗЛО ЗАЛЕЗЛИ
        "%-2%+Ь", "%-2%+ЬТЕ"                        // ЗАЛЕЗЬ ЗАЛЕЗЬТЕ
       )

 глагол2(                                                   // ПРИБАВИТЬСЯ
         СОВЕРШ, 7082,                                      // ---------------------
         Прибавиться, "(.+)ВИТЬСЯ",
         "%-5%+ЛЮСЬ",   "%-4%+МСЯ",                         // ПРИБАВЛЮСЬ ПРИБАВИМСЯ
         "%-4%+ШЬСЯ", "%-4%+ТЕСЬ",                          // ПРИБАВИШЬСЯ ПРИБАВИТЕСЬ
         "%-4%+ТСЯ",  "%-5%+ЯТСЯ",                          // ПРИБАВИТСЯ ПРИБАВЯТСЯ
         "%-4%+ЛСЯ", "%-4%+ЛАСЬ", "%-4%+ЛОСЬ", "%-4%+ЛИСЬ", // ПРИБАВИЛСЯ ПРИБАВИЛАСЬ ПРИБАВИЛОСЬ ПРИБАВИЛИСЬ
         "%-5%+ЬСЯ", "%-5%+ЬТЕСЬ"                           // ПРИБАВЬСЯ ПРИБАВЬТЕСЬ
        )

 глагол2(                                               // ЗАРЕЗАТЬ
         СОВЕРШ, 7083,                                  // --------------------------
         Зарезать, "(.+)ЗАТЬ",
         "%-4%+ЖУ",   "%-4%+ЖЕМ",                       // ЗАРЕЖУ ЗАРЕЖЕМ
         "%-4%+ЖЕШЬ", "%-4%+ЖЕТЕ",                      // ЗАРЕЖЕШЬ ЗАРЕЖЕТЕ
         "%-4%+ЖЕТ",  "%-4%+ЖУТ",                       // ЗАРЕЖЕТ ЗАРЕЖУТ
         "%-2%+Л", "%-2%+ЛА", "%-2%+ЛО", "%-2%+ЛИ",     // ЗАРЕЗАЛ ЗАРЕЗАЛА ЗАРЕЗАЛО ЗАРЕЗАЛИ
         "%-4%+ЖЬ", "%-4%+ЖЬТЕ"                         // ЗАРЕЖЬ ЗАРЕЖЬТЕ
        )

 глагол2(                                                   // СКОНТАЧИТЬСЯ
         СОВЕРШ, 7084,                                      // --------------------------
         СКОНТАЧИТЬСЯ, "(.+)ЧИТЬСЯ",
         "%-5%+УСЬ",   "%-4%+МСЯ",                          // СКОНТАЧУСЬ СКОНТАЧИМСЯ
         "%-4%+ШЬСЯ", "%-4%+ТЕСЬ",                          // СКОНТАЧИШЬСЯ СКОНТАЧИТЕСЬ
         "%-4%+ТСЯ",  "%-5%+АТСЯ",                          // СКОНТАЧИТСЯ СКОНТАЧАТСЯ
         "%-4%+ЛСЯ", "%-4%+ЛАСЬ", "%-4%+ЛОСЬ", "%-4%+ЛИСЬ", // СКОНТАЧИЛСЯ СКОНТАЧИЛАСЬ СКОНТАЧИЛОСЬ СКОНТАЧИЛИСЬ
         "%-5%+ЬСЯ", "%-5%+ЬТЕСЬ"                           // СКОНТАЧЬСЯ СКОНТАЧЬТЕСЬ
        )
        
}
