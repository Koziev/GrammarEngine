 #define ADJECTIVE_1(                               \
                     NAME,                          \
                     F1, F2, F3, F4,                \
                     F5, F6, F7, F8,                \
                     F9, F10, F11, F12, F13,        \
                     F14, F15, F16, F17, F18, F19,  \
                     F20, F21, F22, F23,            \
                     F24, F25, F26, F27             \
 )                               \
 #begin
 entry NAME : ПРИЛАГАТЕЛЬНОЕ
 {
  СТЕПЕНЬ:АТРИБ |{ ~КРАТКИЙ |{

  ПАДЕЖ:(ИМ) |{
               ЧИСЛО:ЕД РОД { F1 F2 F3 }
               ЧИСЛО:МН     { F4 }
              }

  ПАДЕЖ:(РОД) |{
                ЧИСЛО:ЕД РОД { F5 F6 F7 }
                ЧИСЛО:МН     { F8 }
               }

  ПАДЕЖ:ТВОР |{
               ЧИСЛО:ЕД РОД     { F9 F10 F11 }
               ЧИСЛО:ЕД РОД:ЖЕН {      F12   }
               ЧИСЛО:МН         { F13        }
              }

  ПАДЕЖ:ВИН |{
              ЧИСЛО:ЕД РОД:МУЖ ОДУШ { F14 F15 }
              ЧИСЛО:ЕД РОД:ЖЕН      { F16     }
              ЧИСЛО:ЕД РОД:СР       { F17     }
              ЧИСЛО:МН ОДУШ         { F18 F19 }
             }

  ПАДЕЖ:ДАТ |{
              ЧИСЛО:ЕД РОД { F20 F21 F22 }
              ЧИСЛО:МН     { F23 }
             }

  ПАДЕЖ:(ПРЕДЛ) |{
                  ЧИСЛО:ЕД РОД { F24 F25 F26 }
                  ЧИСЛО:МН     { F27 }
                 }

  } }
 }
 #end

